<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to image_tag('tsu.png', width: '300', height: '75'), root_path, id: 'logo' %>
    <nav>
      <div class="navbar-header">
        <button id="hamburger" type="button" class="navbar-toggle collapsed">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
      </div>
      <ul id="navbar-menu" class="nav navbar-nav navbar-right collapse navbar-collapse">
        <li><%= link_to "Home", root_path, id: 'ez_menu' %></li>
        <li><%= link_to "Help", help_path, id: 'ez_menu' %></li>
        <li><%= link_to "About", about_path, id: 'ez_menu' %></li>
        <li><%= link_to "Contact", contact_path, id: 'ez_menu' %></li>
        <% if logged_in? %>
          <li><% if current_user.admin? %><%= link_to "Agents", users_path, id: 'ez_menu' %><% end %></li>
          <li class="dropdown" id="ez_menu">
            <a href="#" id="account" class="dropdown-toggle" style="color: #FFF">
              Account <b class="caret"></b>
            </a>
            <ul id="dropdown-menu" class="dropdown-menu">
              <li><%= link_to "Profile", current_user %></li>
              <li><%= link_to "Edit Profile", edit_user_path(current_user) %></li>
              <li class="divider"></li>
              <li>
                <%= link_to "Log out", logout_path, data: { "turbo-method": :delete } %>
              </li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Log in", login_path, id: 'ez_menu' %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header><br/>
<br/>
